{
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 120,
  "semi": true,
  "trailingComma": "all",
  "endOfLine": "lf",
  "bracketSpacing": true,
  "arrowParens": "always",
  "quoteProps": "consistent",
  "proseWrap": "preserve",
  "embeddedLanguageFormatting": "auto",
  "arrayBracketNewline": true,
  "objectCurlyNewline": true
}
